{
  "printWidth": 80,
  "singleQuote": true,
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "all",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": ["<THIRD_PARTY_MODULES>", "^@(.*)$", "", "^\\.(.*)$"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
